@model Final_WebApplication_Admin.Models.TrainingCategory;
@{
    Layout = "_Layout";
}

<h4>Training Entity Form</h4>
<hr />
<form asp-controller="Training" asp-action="Add_Category" enctype="multipart/form-data" method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="Category_Name" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="Category_Name" class="form-control mb-2" />
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Add_Category" class="btn btn-primary bm-2" />
    </div>
</form>
<div>
    <a asp-asp-controller="Training" asp-action="GetAllCategory">Back To List</a>
</div>
