@model Final_WebApplication_Admin.Models.AppUser;
@{
    Layout = "_Layout";
}

<h4>User Entity Form</h4>
<hr />
<form asp-controller="User" asp-action="AddUser" enctype="multipart/form-data" method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="UserName" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="UserName" class="form-control mb-2" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Email" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="Email" class="form-control mb-2" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="Password" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input type="password" asp-for="Password" class="form-control mb-2" />
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="AddUser" class="btn btn-primary bm-2" />
    </div>
</form>
<div>
    <a asp-asp-controller="User" asp-action="GetAllUsers">Back To List</a>
</div>
